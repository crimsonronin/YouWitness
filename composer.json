{
    "name": "crimsonronin/YouWitness",
    "description": "You Witness",
    "license": "",
    "keywords": [
        "zoop",
        "zf2"
    ],
    "homepage": "http://youwitness.com.au/",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://raw.github.com/superdweebie/dojo/master"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.*",
        "doctrine/doctrine-module": "0.7.*",
        "doctrine/doctrine-orm-module": "dev-master",
        "superdweebie/dojo": "dev-master",
        "superdweebie/sijit": "dev-master",
        "xsokev/dojo-bootstrap": "v1.2",
        "twitter/bootstrap": "2.3.1",
        "fort-awesome/font-awesome": "3.0.1"
    },
    "autoload": {
        "psr-0": {
            "YouWitness\\": "module/YouWitness/src"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/dojo/Sds": ["superdweebie/sijit"],
            "vendor/dojo/bootstrap": ["xsokev/dojo-bootstrap"],
            "vendor/dojo/dijit": ["dojo/dijit"],
            "vendor/dojo/dojo": ["dojo/dojo"],
            "vendor/dojo/dojox": ["dojo/dojox"],
            "vendor/dojo/util": ["dojo/util"]
        }
    }
}